{
  "name": "responsive-photo-gallery",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "IMAGE_PATH=./pics AUTH_PATH=./data/auth THUMB_PATH=./data/thumbs PRIVATE_KEY=nonvolatile PORT=8000 DEBUG=express,responsive-photo-gallery:* npm start",
    "build-debug-frontend": "PUBLIC_URL=./ npm run build-frontend",
    "build-frontend": "cd frontend; npm run build; cd .."
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "exif-reader": "^1.0.2",
    "express": "~4.15.5",
    "jsonwebtoken": "^8.1.0",
    "morgan": "~1.9.0",
    "nedb": "^1.8.0",
    "pug": "2.0.0-beta11",
    "sanitize-filename": "^1.6.1",
    "serve-favicon": "~2.4.5",
    "sharp": "^0.18.4",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^2.0.9"
  },
  "description": "NodeJS backend to serve a photo collection in a mobile-friendly and beautiful way",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwater7/responsive-photo-gallery.git"
  },
  "keywords": [
    "photo",
    "picture",
    "image",
    "gallery",
    "album"
  ],
