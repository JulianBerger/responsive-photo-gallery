{
  "name": "responsive-photo-gallery",
  "version": "0.2.8",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "IMAGE_PATH=./debug-data/pics AUTH_PATH=./debug-data/data/auth PRIVATE_KEY=nonvolatile THUMB_PATH=./debug-data/data/thumbs PORT=8000 DEBUG=express,responsive-photo-gallery:* npm start",
