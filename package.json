    "passport": "^0.4.1",
    "passport-jwt-cookiecombo": "^1.0.5",
    "pug": "2.0.0-beta11",
    "query-string": "^6.0.0",
    "sanitize-filename": "^1.6.1",
    "serve-favicon": "~2.4.5",
    "swagger-jsdoc": "^1.9.7",
